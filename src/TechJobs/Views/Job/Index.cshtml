@{
    // TODO #2.1- declare the ViewModel (outside of this @{ } block)
}
@model List<TechJobs.Models.Job>

<table class="job-listing">
    @foreach (var job in Model)
    {
        <tr>
            <td>Name</td>
            <td>@job.Name</td>
        </tr>
        <tr>
            <td>Employer</td>
            <td>@job.Employer</td>
        </tr>
        <tr>
            <td>Location</td>
            <td>@job.Location</td>
        </tr>
        <tr>
            <td>Skill</td>
            <td>@job.CoreCompetency</td>
        </tr>
        <tr>
            <td>Position Type</td>
            <td>@job.PositionType</td>
        </tr>
    }
</table>